<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="repository.dosen.models.TranProgressFunctional" table="TranProgressFunctional">
    <id column="idTranProgress" name="idTranProgress">
      <generator class="native"/>
    </id>
    <property column="state" name="state"/>
    <many-to-one class="repository.dosen.models.DetailProgress" column="idDetail" name="detail" not-null="true"/>
    <many-to-one class="repository.dosen.models.FunctionalProgressHistory" column="idProgressHistory" name="progressHistory"/>
  </class>
</hibernate-mapping>
