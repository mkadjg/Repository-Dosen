<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="repository.dosen.models.MasterStudy" table="MasterStudy">
        <id name="idStudy" column="idStudy">
            <generator class="native"/>
        </id>
        <property name="studyDescription" column="studyDescription"/>
    </class>
</hibernate-mapping>
